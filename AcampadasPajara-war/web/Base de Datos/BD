-----------------------------------------------------------------------------------------------------------------------
CREATE TABLE PLAYAPLAZASFECHAOCUPADAS (PPFO INTEGER GENERATED ALWAYS AS IDENTITY PRIMARY KEY, PLAYA VARCHAR(144) NOT NULL, FECHA VARCHAR(144) NOT NULL, PLAZAS INTEGER DEFAULT 1  NOT NULL);
-----------------------------------------------------------------------------------------------------------------------

CREATE TABLE PLAYADATOSPERSONAS ((PDP_ID INTEGER GENERATED ALWAYS AS IDENTITY PRIMARY KEY, PLAYA VARCHAR(144) NOT NULL, NOMBREPERSONA VARCHAR(144) NOT NULL, APELLIDO VARCHAR(144) NOT NULL, DNI VARCHAR(144) NOT NULL, MUNICIPIO VARCHAR(144) NOT NULL, EMAIL VARCHAR(144) NOT NULL, TELEFONO VARCHAR(144) NOT NULL, FAX VARCHAR(144), FECHA_ENTRADA VARCHAR(144) NOT NULL, FECHA_SALIDA VARCHAR(144) NOT NULL, NUM_PERSONAS INTEGER DEFAULT 1  NOT NULL, TIPO_ACAMP VARCHAR(144) NOT NULL, NUM_CASETAS INTEGER DEFAULT 0  NOT NULL, MATRICULA_CAR VARCHAR(144), RECOGER VARCHAR(144) NOT NULL, OBSERVACIONES LONG VARCHAR);

-----------------------------------------------------------------------------------------------------------------------
CREATE TABLE HISTORIAL (HIST_ID INTEGER DEFAULT AUTOINCREMENT: start 1 increment 1  NOT NULL GENERATED ALWAYS AS IDENTITY, PLAYA VARCHAR(144) NOT NULL, NOMBREPERSONA VARCHAR(144) NOT NULL, APELLIDO VARCHAR(144) NOT NULL, DNI VARCHAR(144) NOT NULL, FECHA_ENTRADA VARCHAR(144) NOT NULL, FECHA_SALIDA VARCHAR(144) NOT NULL, PRIMARY KEY (HIST_ID));


				-------BORRAR-----
-----------------------------------------------------------------------------------------------------------------------
CREATE TABLE MUNICIPIOS_PROVISIONALES (MUNI_PROV_ID INTEGER DEFAULT AUTOINCREMENT: start 1 increment 1  NOT NULL GENERATED ALWAYS AS IDENTITY, MUNI_PROVISIONAL_ID VARCHAR(144) NOT NULL, PRIMARY KEY (MUNI_PROV_ID));
-----------------------------------------------------------------------------------------------------------------------
CREATE TABLE MUNICIPIOS (MUNI_ID INTEGER DEFAULT AUTOINCREMENT: start 1 increment 1  NOT NULL GENERATED ALWAYS AS IDENTITY, MUNICIPIO VARCHAR(144) NOT NULL, PRIMARY KEY (MUNI_ID));

